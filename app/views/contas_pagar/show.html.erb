<div class="page-header">
  <%= link_to contas_pagar_index_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_contas_pagar_path(@contas_pagar), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar</h1>
</div>

<dl class="dl-horizontal">
  <dt>Fornecedores:</dt>
  <dd><%= @contas_pagar.fornecedores %></dd>
  <dt>Plano contas:</dt>
  <dd><%= @contas_pagar.plano_contas %></dd>
  <dt>Documento:</dt>
  <dd><%= @contas_pagar.documento %></dd>
  <dt>Historico:</dt>
  <dd><%= @contas_pagar.historico %></dd>
  <dt>Data emissao:</dt>
  <dd><%= @contas_pagar.data_emissao %></dd>
  <dt>Valor total:</dt>
  <dd><%= @contas_pagar.valor_total %></dd>
</dl>
