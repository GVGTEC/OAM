<div class="page-header">
  <%= link_to cliente_contatos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_cliente_contato_path(@cliente_contato), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar</h1>
</div>

<dl class="dl-horizontal">
  <dt>Cliente:</dt>
  <dd><%= @cliente_contato.cliente %></dd>
  <dt>Nome:</dt>
  <dd><%= @cliente_contato.nome %></dd>
  <dt>Telefone:</dt>
  <dd><%= @cliente_contato.telefone %></dd>
  <dt>Email:</dt>
  <dd><%= @cliente_contato.email %></dd>
</dl>
