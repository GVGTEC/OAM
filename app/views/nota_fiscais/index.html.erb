<div class="page-header">
  <%= link_to new_nota_fiscal_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    Novo Nota fiscal
  <% end %>
  <h1>Lista</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Numero nota</th>
            <th>Numero pedido</th>
            <th>Cfop</th>
            <th>Entsai</th>
            <th>Cliente</th>
            <th>Fornecedor</th>
            <th>Vendedor</th>
            <th>Data emissao</th>
            <th>Data saida</th>
            <th>Hora saida</th>
            <th>Valor desconto</th>
            <th>Valor produtos</th>
            <th>Valor total nota</th>
            <th>Valor frete</th>
            <th>Valor outras despesas</th>
            <th>Numero pedido compra</th>
            <th>Tipo pagamento</th>
            <th>Meio pagamento</th>
            <th>Numero parcelas pagamento</th>
            <th>Observacao</th>
            <th>Chave acesso nfe</th>
            <th>Nota cancelada sn</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @nota_fiscais) do |nota_fiscal| %>
            <td><%= nota_fiscal.numero_nota %></td>
            <td><%= nota_fiscal.numero_pedido %></td>
            <td><%= nota_fiscal.cfop %></td>
            <td><%= nota_fiscal.entsai %></td>
            <td><%= nota_fiscal.cliente %></td>
            <td><%= nota_fiscal.fornecedor %></td>
            <td><%= nota_fiscal.vendedor %></td>
            <td><%= nota_fiscal.data_emissao %></td>
            <td><%= nota_fiscal.data_saida %></td>
            <td><%= nota_fiscal.hora_saida %></td>
            <td><%= nota_fiscal.valor_desconto %></td>
            <td><%= nota_fiscal.valor_produtos %></td>
            <td><%= nota_fiscal.valor_total_nota %></td>
            <td><%= nota_fiscal.valor_frete %></td>
            <td><%= nota_fiscal.valor_outras_despesas %></td>
            <td><%= nota_fiscal.numero_pedido_compra %></td>
            <td><%= nota_fiscal.tipo_pagamento %></td>
            <td><%= nota_fiscal.meio_pagamento %></td>
            <td><%= nota_fiscal.numero_parcelas_pagamento %></td>
            <td><%= nota_fiscal.observacao %></td>
            <td><%= nota_fiscal.chave_acesso_nfe %></td>
            <td><%= nota_fiscal.nota_cancelada_sn %></td>
            <td><%= link_to 'Mostrar', nota_fiscal %></td>
        <td><%= link_to 'Editar', edit_nota_fiscal_path(nota_fiscal) %></td>
        <td><%= link_to 'Apagar', nota_fiscal, method: :delete, data: { confirm: 'Confirma ?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
